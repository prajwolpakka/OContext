{
	"name": "ocontext",
	"displayName": "OContext",
	"description": "Generate LLM prompts from selected files/folders",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ocontext.generatePromptToFile",
		"onCommand:ocontext.generatePromptToClipboard"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ocontext.generatePromptToFile",
				"title": "OContext: Generate Context"
			},
			{
				"command": "ocontext.generatePromptToClipboard",
				"title": "OContext: Generate Context and Copy to Clipboard"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "ocontext.generatePromptToFile",
					"group": "2_workspace"
				},
				{
					"command": "ocontext.generatePromptToClipboard",
					"group": "2_workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/vscode": "^1.74.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"ignore": "^7.0.5"
	}
}
